{
  "name": "@ng-rspack/build",
  "private": false,
  "description": "Rspack Plugin and Loaders for building Angular.",
  "author": "Colum Ferry",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Coly010/ng-rspack-build/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Coly010/ng-rspack-build.git",
    "directory": "packages/build"
  },
  "keywords": [
    "Monorepo",
    "Angular",
    "Rspack"
  ],
  "version": "0.0.1",
  "scripts": {
    "postinstall": "node ./src/lib/patch/patch-angular-build.js"
  },
  "dependencies": {
    "@swc/helpers": "~0.5.11",
    "sass-loader": "^16.0.2",
    "sass-embedded": "^1.79.3",
    "express": "4.21.1",
    "http-proxy-middleware": "3.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21"
  },
  "peerDependencies": {
    "@rspack/core": ">=1.0.5 <2.0.0",
    "@rsbuild/core": ">=1.0.5 <2.0.0",
    "@rsbuild/plugin-sass": ">=1.0.5 <2.0.0",
    "@angular/build": ">=18.0.0 <20.0.0",
    "sass": "^1.79.3",
    "@module-federation/enhanced": "^0.8.5",
    "@module-federation/sdk": "^0.8.5",
    "@nx/devkit": ">=20.0.0 <21.0.0",
    "@nx/js": ">=20.0.0 <21.0.0",
    "nx": ">=20.0.0 <21.0.0"
  },
  "type": "commonjs",
  "main": "./src/index.js",
  "typings": "./src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/index.js"
    },
    "./rspack": {
      "types": "./src/lib/rspack/index.d.ts",
      "default": "./src/lib/rspack/index.js"
    },
    "./rspack/loaders/hmr-loader": {
      "types": "./src/lib/rspack/loaders/hmr/hmr-loader.d.ts",
      "default": "./src/lib/rspack/loaders/hmr/hmr-loader.js"
    },
    "./rspack/loaders/js-loader": {
      "types": "./src/lib/rspack/loaders/js-loader.d.ts",
      "default": "./src/lib/rspack/loaders/js-loader.js"
    },
    "./rspack/loaders/angular-loader": {
      "types": "./src/lib/rspack/loaders/angular-loader.d.ts",
      "default": "./src/lib/rspack/loaders/angular-loader.js"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
